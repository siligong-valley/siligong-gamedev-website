# Site Configuration
PUBLIC_SITE_URL=https://gamedev.siligongvalley.com

# API Endpoints
PUBLIC_NEWSLETTER_ENDPOINT=https://api.example.com/newsletter
PUBLIC_CONTACT_ENDPOINT=https://api.example.com/contact
PUBLIC_EVENTS_ENDPOINT=https://api.example.com/events

# Third-party Services (keep private - no PUBLIC_ prefix)
NEWSLETTER_API_KEY=your_newsletter_api_key_here
CONTACT_FORM_API_KEY=your_contact_form_api_key_here

# Analytics (optional)
PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Social Media (optional overrides for config.ts)
PUBLIC_DISCORD_INVITE=https://discord.gg/your-invite
PUBLIC_MEETUP_GROUP=https://www.meetup.com/your-group

# Feature Flags (optional overrides)
PUBLIC_ENABLE_NEWSLETTER=true
PUBLIC_ENABLE_CONTACT_FORM=true
PUBLIC_ENABLE_EVENTS=true
